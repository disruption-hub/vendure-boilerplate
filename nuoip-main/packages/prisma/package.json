{
  "name": "@ipnuo/prisma",
  "version": "0.1.0",
  "private": true,
  "main": "src/index.ts",
  "types": "src/index.ts",
  "scripts": {
    "generate": "prisma generate --schema ./schema.prisma",
    "migrate:dev": "prisma migrate dev --schema ./schema.prisma",
    "migrate:deploy": "prisma migrate deploy --schema ./schema.prisma",
    "migrate:status": "prisma migrate status --schema ./schema.prisma || true",
    "seed": "ts-node seed.ts",
    "studio": "prisma studio --schema ./schema.prisma"
  },
  "dependencies": {
    "@prisma/client": "^7.0.0",
    "@prisma/adapter-pg": "^7.0.0",
    "pg": "^8.11.3"
  },
  "devDependencies": {
    "prisma": "^7.0.0",
    "ts-node": "^10.9.2",
    "typescript": "^5.6.3"
  }
}