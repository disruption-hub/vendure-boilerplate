{
  "name": "ip-nuo",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev --webpack",
    "build": "(npm run prisma:generate || true) && next build",
    "start": "next start",
    "start:local": "NEXT_PUBLIC_BACKEND_URL=http://localhost:3001 BACKEND_URL=http://localhost:3001 next start",
    "lint": "next lint",
    "prisma:generate": "if [ -f ./schema.prisma ]; then npx prisma generate --schema ./schema.prisma; elif [ -f ../../packages/prisma/schema.prisma ]; then npx prisma generate --schema ../../packages/prisma/schema.prisma; else echo 'Prisma schema not found, skipping generation'; fi"
  },
